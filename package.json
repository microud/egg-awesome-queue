{
  "name": "egg-awesome-queue",
  "version": "0.0.1",
  "description": "Wrapper of bull for egg.",
  "eggPlugin": {
    "name": "queue"
  },
  "types": "./index.d.ts",
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {
    "@types/bull": "^3.12.1",
    "bull": "^3.13.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/reflect-metadata": "^0.1.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.16.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
